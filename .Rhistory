# Calcul des limites de surveillance
limites_xbar_surveillance <- with(carte_xbar, c(center - 2 * std.dev, center + 2 * std.dev))
limites_r_surveillance <- with(carte_r, c(center - 2 * std.dev, center + 2 * std.dev))
limites_c_surveillance <- with(carte_c, c(center - 2 * sqrt(center), center + 2 * sqrt(center)))
# Affichage des cartes avec les limites de surveillance
plot(carte_xbar, limits = limites_xbar_surveillance, title = "Carte Xbar avec limites de surveillance")
plot(carte_r, limits = limites_r_surveillance, title = "Carte R avec limites de surveillance")
plot(carte_c, limits = limites_c_surveillance, title = "Carte C avec limites de surveillance")
